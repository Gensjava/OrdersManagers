<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/LinearLayoutmain"
    xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_weight="0.33"
            android:gravity="center|fill_vertical"
            android:text="Цена"/>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_weight="0.33"
            android:gravity="center"
            android:text="Количество"/>

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_weight="0.33"
            android:autoText="false"
            android:gravity="center"
            android:text="Сумма"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_gravity="center_horizontal"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/dialog_number_price"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_weight="0.25"
            android:gravity="center_vertical|center_horizontal"
            android:text="price"/>

        <ImageView
            android:id="@+id/dialog_number_minus"
            android:layout_width="40dp"
            android:layout_height="35dp"
            android:gravity="center"
            android:onClick="onClick"
            android:src="@mipmap/ic_dialog_minus"/>

        <EditText
            android:id="@+id/dialog_number_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            android:clickable="false"
            android:focusable="true"
            android:gravity="center_vertical|center_horizontal"
            android:inputType=""
            android:longClickable="true"
            android:text="1.0"/>

        <ImageView
            android:id="@+id/dialog_number_plus"
            android:layout_width="40dp"
            android:layout_height="35dp"
            android:gravity="center"
            android:onClick="onClick"
            android:src="@mipmap/ic_dialog_plus"/>

        <TextView
            android:id="@+id/dialog_number_sum"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_weight="0.25"
            android:autoText="false"
            android:gravity="center_vertical|center_horizontal"
            android:text="Sum"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutmainOne"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginBottom="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <Button
            android:id="@+id/but_one"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog"
            android:contentDescription="1"
            android:onClick="OnClickNamber"
            android:text="1"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/but_two"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog"
            android:contentDescription="2"
            android:onClick="OnClickNamber"
            android:singleLine="false"
            android:text="2"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/but_tree"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog"
            android:contentDescription="3"
            android:onClick="OnClickNamber"
            android:text="3"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/but_del"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog_main_grey"
            android:onClick="OnClickNamber"
            android:singleLine="false"
            android:text="&lt;-"
            android:textColor="#ffffff"
            android:textSize="20dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutmainTwo"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_margin="2dp"
        android:orientation="horizontal"
        >

        <Button
            android:id="@+id/but_four"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog"
            android:contentDescription="4"
            android:onClick="OnClickNamber"
            android:singleLine="true"
            android:text="4"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/but_five"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog"
            android:contentDescription="5"
            android:onClick="OnClickNamber"
            android:singleLine="false"
            android:text="5"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/but_six"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog"
            android:contentDescription="6"
            android:onClick="OnClickNamber"
            android:text="6"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/but_dumping"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog_main_grey"
            android:onClick="OnClickNamber"
            android:singleLine="false"
            android:text="C"
            android:textColor="#ffffff"
            android:textSize="20dp"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_margin="2dp"
        android:orientation="horizontal"
        >

        <Button
            android:id="@+id/but_seven"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog"
            android:contentDescription="7"
            android:onClick="OnClickNamber"
            android:singleLine="false"
            android:text="7"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/but_eight"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog"
            android:contentDescription="8"
            android:onClick="OnClickNamber"
            android:singleLine="false"
            android:text="8"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/but_nine"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog"
            android:contentDescription="9"
            android:onClick="OnClickNamber"
            android:text="9"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/but_point"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog_main_grey"
            android:singleLine="false"
            android:textColor="#ffffff"
            android:textSize="20dp"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_margin="2dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal">

        <Button
            android:id="@+id/button9"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog_main_grey"
            android:singleLine="true"
            android:textColor="@color/abc_background_cache_hint_selector_material_light"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/but_zero"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog"
            android:contentDescription="0"
            android:onClick="OnClickNamber"
            android:singleLine="false"
            android:text="0"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/button11"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog_main_grey"
            android:singleLine="true"
            android:textColor="@color/abc_background_cache_hint_selector_material_light"
            android:textSize="20dp"/>

        <Button
            android:id="@+id/button12"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_margin="2dp"
            android:layout_weight="0.25"
            android:background="@drawable/roundbutton_dialog_main_grey"
            android:singleLine="false"
            android:textColor="@color/abc_background_cache_hint_selector_material_light"
            android:textSize="20dp"/>
    </LinearLayout>

</LinearLayout>